					<Item label="Switch is on" value="1" />
					<Item label="Switch is off" value="2" />
				</Value>
				<Value type="list" genre="config" instance="1" index="80" label="Lifeline Notification report" units="" read_only="false" write_only="false" verify_changes="false" poll_intensity="0" min="0" max="2" vindex="2" size="1">
					<Help>Defines the automated status notification of an associated device when status changes</Help>
